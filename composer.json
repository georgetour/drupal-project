{
    "name": "georgetour/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
      {
          "name": "George Tourtsinakis",
          "email": "george@besmartbesimple.com",
          "role": "Maintainer"
      }
    ],
    "support": {
      "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
      "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
      { "type": "composer",
        "url": "https://packages.drupal.org/8"
      },
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ],
    "require": {
      "bower-asset/blazy": "^1.8",
      "bower-asset/ckeditor-image-button": "^1.0.0",
      "bower-asset/dropzone": "4.3.0",
      "bower-asset/imagesloaded": "^4.1",
      "bower-asset/intl-tel-input": "^12.1.12",
      "bower-asset/jquery.inputmask": "5.0.5",
      "bower-asset/masonry": "^4.2",
      "bower-asset/photoswipe": "^4.1",
      "bower-asset/progress-tracker": "^1.4",
      "bower-asset/select2": "^4.0.5",
      "bower-asset/slick-carousel": "^1.7",
      "bower-asset/timepicker": "^1.11.13",
      "composer/installers": "^1.9",
      "cweagans/composer-patches": "^1.6",
      "drupal/admin_toolbar": "^3.0",
      "drupal/better_exposed_filters": "^5.0",
      "drupal/blazy_photoswipe": "*",
      "drupal/block_content_permissions": "^1.6",
      "drupal/config_ignore": "^2.1",
      "drupal/console": "^1.0.2",
      "drupal/core-composer-scaffold": "^8.8.0",
      "drupal/core-project-message": "^8.8.0",
      "drupal/core-recommended": "^8.8.0",
      "drupal/default_content": "^2.0@alpha",
      "drupal/devel": "^1.2",
      "drupal/dropzonejs": "^2.0",
      "drupal/drush_language": "^1.0@RC",
      "drupal/easy_breadcrumb": "^1.8",
      "drupal/editor_advanced_link": "^1.4",
      "drupal/editor_file": "^1.4",
      "drupal/embed": "*",
      "drupal/empty_page": "^3.0",
      "drupal/entity_browser": "^2.0",
      "drupal/entity_embed": "^1.0",
      "drupal/entity_usage": "^2.0@beta",
      "drupal/field_group": "^1.0",
      "drupal/file_browser": "^1.1",
      "drupal/file_mdm": "^2.1",
      "drupal/google_analytics": "^2.2",
      "drupal/google_tag": "^1.1",
      "drupal/honeypot": "^2.0",
      "drupal/image_effects": "^3.0",
      "drupal/image_style_quality": "^1.3",
      "drupal/inline_entity_form": "^1.0",
      "drupal/jquery_ui": "^1.4",
      "drupal/lang_dropdown": "^2.0",
      "drupal/layout_builder_styles": "^1.0@beta",
      "drupal/link_attributes": "^1.2",
      "drupal/linkit": "^4.3",
      "drupal/mailsystem": "^4.1",
      "drupal/media_entity_browser": "^2.0",
      "drupal/metatag": "^1.5",
      "drupal/paragraphs": "^1.2",
      "drupal/pathauto": "^1.6@beta",
      "drupal/photoswipe": "^1.0",
      "drupal/recaptcha": "^2.3",
      "drupal/redirect": "^1.4",
      "drupal/search_api": "^1.8",
      "drupal/shield": "^1.2",
      "drupal/simple_popup_blocks": "^1.2",
      "drupal/simple_sitemap": "^3.0",
      "drupal/slick": "^2.0",
      "drupal/slick_media": "^3.0",
      "drupal/slick_views": "^2.0@RC",
      "drupal/smtp": "^1.0@beta",
      "drupal/token": "^1.1",
      "drupal/twig_tweak": "^2.0",
      "drupal/video_embed_field": "^2.0",
      "drupal/views_bulk_operations": "*",
      "drupal/views_field_view": "*",
      "drupal/views_infinite_scroll": "*",
      "drupal/webform": "^5.0@RC",
      "drupal/webform_layout_container": "*",
      "drush/drush": "^10.0.0",
      "npm-asset/ckeditor-autogrow": "^1.0",
      "npm-asset/jquery-ui-touch-punch": "^0.2.3",
      "onlynet/twigimage": "^0.0.1",
      "oomphinc/composer-installers-extender": "^2.0",
      "webflo/drupal-finder": "^1.0.0",
      "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
      "drupal/core-dev": "^8.9.0"
    },
    "conflict": {
      "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true,
      "discard-changes": true
    },
    "autoload": {
      "psr-4": {
          "DrupalTemplate\\composer\\": "src/composer/"
      }
    },
    "scripts": {
      "post-install-cmd": [
        "DrupalTemplate\\composer\\Script::createRequiredFiles"
      ],
      "post-update-cmd": [
        "DrupalTemplate\\composer\\Script::createRequiredFiles"
      ]
    },
    "extra": {
      "installer-types": [
        "bower-asset",
        "npm-asset"
      ],
      "drupal-scaffold": {
        "locations": {
          "web-root": "public_html"
        },
        "file-mapping": {
          "[web-root]/robots.txt": false,
          "[web-root]/sites/development.services.yml": false,
          "[web-root]/.eslintrc.json": false
        }
      },
      "merge-plugin": {
        "include": [
          "public_html/modules/contrib/webform/composer.libraries.json"
        ]
      },
      "installer-paths": {
        "public_html/core": [
          "type:drupal-core"
        ],
        "public_html/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "public_html/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "public_html/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/Commands/contrib/{$name}": [
          "type:drupal-drush"
        ],
        "public_html/modules/onlynet/{$name}": [
          "type:drupal-custom-module"
        ],
        "public_html/libraries/{$name}": [
          "type:drupal-library",
          "type:bower-asset",
          "type:npm-asset",
          "vendor:npm-asset",
          "vendor:bower-asset"
        ],
        "public_html/themes/{$name}": [
          "type:drupal-custom-theme"
        ]
      },
      "drupal-core-project-message": {
        "include-keys": [
          "homepage",
          "support"
        ],
        "post-create-project-cmd-message": [
          "<bg=blue;fg=white>                                                         </>",
          "<bg=blue;fg=white>  Congratulations, you've installed the Drupal codebase  </>",
          "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
          "<bg=blue;fg=white>                                                         </>",
          "",
          "<bg=yellow;fg=black>Next steps</>:",
          "  * Install the site: https://www.drupal.org/docs/8/install",
          "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
          "  * Get support: https://www.drupal.org/support",
          "  * Get involved with the Drupal community:",
          "      https://www.drupal.org/getting-involved",
          "  * Remove the plugin that prints this message:",
          "      composer remove drupal/core-project-message"
        ]
      },
      "patches": {
        "drupal/core": {
          "Drupal\\language\\LanguageNegotiator::updateConfiguration() is passed a NULL": "https://www.drupal.org/files/issues/language-2718657-3.patch",
          "config sync error on import after removing base field and it's core.base_field_override*": "https://www.drupal.org/files/issues/2019-05-21/config_import_error-2999487-7.patch",
          "Fatal error on config form with translation enabled when config is missing": "https://www.drupal.org/files/issues/2019-09-26/core-typed_config_handle_missing_config-2925297-18.patch",
          "dblog_update_8600() breaks views.view.watchdog config entity, leading to PHP warning on admin/reports/dblog and configuration import errors": "https://www.drupal.org/files/issues/2020-02-04/drupal-n2998103-92.patch",
          "TypeError: Argument 1 passed to Drupal/views/Plugin/views/field/EntityOperations::getEntityTranslation()" : "https://www.drupal.org/files/issues/2021-01-21/3105003-9.patch"
        },
        "drupal/entity_browser:" : {
          "Fatal error when using the entity browser widget on a base field" : "https://www.drupal.org/files/issues/2019-07-18/3068557-cannot-add-widget-in-any-form.patch"
        }
      }
    }
  }
  
